/* miniprogram/pages/contact/contact.wxss */
.container{
  display: flex;
  flex-direction: column;
}
.container image{
  width:  100%;
  height: 420rpx;
}
.introText{
  margin: 20rpx;
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
}
.introduction, .tel, .address{ 
  margin-top: 5px;
  width: 100%;
  padding: 5px 0 5px 5px;
  background-color: #eeeeee;
  border-bottom: 1px solid #cccccc;
}
.tel, .address{
  border-top: 1px solid #cccccc;
}
.font{
  font-size: 28rpx;
  margin: 20rpx;
  line-height: 28rpx;
}
#map{
  margin: 20rpx 0;
  box-sizing: border-box;
  border: 1px solid #cccccc;
}